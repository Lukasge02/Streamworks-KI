      <Script><![CDATA[-- Libraries
--using Streamworks

-- System variables
SysVARI = System.GetSystemVariable('$ST_VARI')


PLDAT=System.GetInternalDate(System.GetSystemVariable('$PD_DDMMYY'),'DDMMYY')
EWT = Streamworks.GetDayOfWeek(PLDAT)
DATUM=System.GetInternalDate('220510','DDMMYY')


-- Run1    Set start time
if System.ToNumber(System.GetSystemVariable('$ST_RUN')) == 1 then
     DAY=System.GetInternalDate(System.GetSystemVariable('$PD_DDMMYY'),'DDMMYY')
     TIME=System.GetInternalDateTime('221109','DDMMYY','191500','HHMMSS','04')
     System.SetStartDate(DAY)
     System.SetStartTime(TIME)
end

-- Run 2   Set start time and set run status of 0040 to inactive
if System.ToNumber(System.GetSystemVariable('$ST_RUN')) == 2 then
     DAY=System.GetInternalDate(System.GetSystemVariable('$PD_DDMMYY'),'DDMMYY')
     TIME=System.GetInternalDateTime('221109','DDMMYY','231500','HHMMSS','04')
     System.SetStartDate(DAY)
     System.SetStartTime(TIME)
	 System.SetJobRunStatus('0040_###_MJob_##','N')
	 
end

-- Run 5   Set Agent to specific AgentName
if System.ToNumber(System.GetSystemVariable('$ST_RUN')) == 5 then
     System.SetAgent('dezirwex')
end


-- Sunday/Run 1  Set start time and set run status of Job 0020 to inactive
if System.GetSystemVariable("$PD_WW") == "Su" then
  if System.ToNumber(System.GetSystemVariable('$ST_RUN')) == 1 then
       DAY=System.GetInternalDate(System.GetSystemVariable('$PD_DDMMYY'),'DDMMYY')
       TIME=System.GetInternalDateTime('221109','DDMMYY','211500','HHMMSS','04')
       System.SetStartDate(DAY)
       System.SetStartTime(TIME)
       System.SetJobRunStatus('0020_###_MJob_##','N')   
  end
end

-- Vari 12345    Set run status of Job 0050 to inactive
if System.GetSystemVariable('$ST_VARI') == "12345" then
     System.SetJobRunStatus('0050_###_MJob_#','N')
end]]></Script>

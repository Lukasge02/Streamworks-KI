    <StreamPreparationScript>
      <Script><![CDATA[-- Timeout f체r die Scriptdurchf체hrung hochgesetzt.
--script_timeout 180

t={}
CurPos=0
MsgTxt=''
-- Definition der Tabelleninhalte und der regul채ren Ausdr체cke
t[1]  = {'Termine f체r ein Jahr l철schen'}
t[2]  = {''}
t[3]  = {'Jahr','','m','^(20\[0-9]{2})$'}

-- Anzeige des Dialogfensters
if (System.ShowDialog('Termine l철schen',t,CurPos,MsgTxt) == false) then
  -- Der Anwender hat Cancel Button gedr체ckt und Eingabe abgebrochen 
  System.Exit('99','Die Eingabe wurde vom Anwender abgebrochen')
end

-- Anzahl Tage im Jahr ermitteln (wegen Schaltjahr) und dann Tabelle Monate erstellen
Jahr=t[3][2]
AnzTageJahr=System.Format(System.GetInternalDate('3112'..Jahr,'DDMMYYYY'),'DDD') -- Ermittlung des julianischen Tags (Tag im Jahr) f체r den 31.12. des vorgegebenen Jahres
System.SetVariable('AnzTageJahr',AnzTageJahr)
AnzTageJahrNum=System.ToNumber(AnzTageJahr)
System.SetVariable('AnzTageJahrNum',AnzTageJahrNum)
if AnzTageJahrNum==365 then
   Monate={31,28,31,30,31,30,31,31,30,31,30,31}
else
   Monate={31,29,31,30,31,30,31,31,30,31,30,31}
end

-- Liste der zu verarbeitenden Streams
Streams={'StrA_TERMIN_001_01','StrA_TERMIN_001_02','StrA_TERMIN_001_03','StrA_TERMIN_001_04'}
tMaxStreams = table.maxn(Streams)

-- Pro Stream pr체fen ob ein Termin gesetzt ist. Wenn Termin gesetzt ist muss ein 'Nicht-Termein' gesetzt werden.
Monat=1 -- Monatsz채hler
tMaxMonate = table.maxn(Monate)
-- Schleife 체ber die Monate
while Monat <= tMaxMonate do
  tMaxTageImMonat = Monate[Monat]
  if Monat < 10 then
     MonatText='0'..Monat
  else
     MonatText=Monat
  end
  System.SetVariable('Monat'..MonatText,MonatText)
  Tag=1
  -- Schleife 체ber die Tage des Monats
  while Tag <= tMaxTageImMonat do
     if Tag < 10 then
        TagText='0'..Tag
     else
        TagText=Tag
     end
     Datum=System.GetInternalDate(TagText..MonatText..Jahr,'DDMMYYYY')
	 Stream = 1
	 -- Schleife 체ber die zu verarbeitenden Streams
	 while Stream <= tMaxStreams do
	    if System.GetScheduleStatus(Streams[Stream],Datum) == true then -- Pr체fen ob f체r den Tag ein Termin gesetzt ist
		   System.SetScheduleStatus(Streams[Stream],Datum,'U')          -- Gesetzter Termin wird entfert
		end
	    Stream=Stream + 1
	 end
     Tag=Tag+1
  end
  Monat = Monat + 1   
end
]]></Script>

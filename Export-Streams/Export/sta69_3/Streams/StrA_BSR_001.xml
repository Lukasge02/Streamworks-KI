-- Wenn in der Auswahl ein SAP System steht, dann dieses SAP System verwenden
if BSR.SAP_Auswahl_01 ~= nil then
   System.SetVariable('SAP_System',BSR.SAP_Auswahl_01)
-- Wenn die Auswahl leer ist und der Anwender manuell ein SAP System vorgegeben hat, dann dieses System verwenden
elseif BSR.SAP_Eingabe_01 ~= nil then
   System.SetVariable('SAP_System',BSR.SAP_Eingabe_01)
-- Wenn beide Felder leer sind bricht die Aufbereitung ab
else
   System.Exit('80','SAP System Name is missing')
end

System.SetStartTime(BSR.Stream_Uhrzeit_01)
System.SetStartDate(BSR.Stream_Datum_01)

KD_Datum=System.Format(BSR.Stream_Datum_01,'YYYY-MM-DD')
KD_Zeit=System.Format(BSR.Stream_Uhrzeit_01,'HH:mm:ss')
KD_Timezone=string.sub(BSR.Stream_Uhrzeit_01,23,24)

SAP_Datum=System.Format(BSR.SAP_Datum_01,'YYYY-MM-DD')
SAP_Zeit=System.Format(BSR.SAP_Uhrzeit_01,'HH:mm:ss')
SAP_Timezone=string.sub(BSR.SAP_Uhrzeit_01,23,24)

if BSR.Warten_01 == 'true' then
   KD_Warten='ja'
else
   KD_Warten='nein'
end

System.SetVariable('KD_Datum',KD_Datum)
System.SetVariable('KD_Zeit',KD_Zeit)
System.SetVariable('KD_Timezone',KD_Timezone)
System.SetVariable('SAP_Datum',SAP_Datum)
System.SetVariable('SAP_Zeit',SAP_Zeit)
System.SetVariable('SAP_Timezone',SAP_Timezone)
System.SetVariable('KD_Warten', KD_Warten)
]]></Script>

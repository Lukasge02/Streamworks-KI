        <ExternalJobScriptRequired IsNull="True" />
        <MainScript><![CDATA[## Aufbereiten des Streams mit Batch Aufruf Blue System Copy
## Im ersten inp Parameter steht der Name incl. Pfad des aufzurufenden Programms
## Danach bilden immer zwei inp Parameter ein Paar. Im 1. wird der Parametername für das aufzurufende Programm und im 2. der dazugehörig Inhalt ausgegeben

StreamworksAgentCLI --port 30090 --cliuser "cli_user_uat_1" --clicmd "func=PREP strname=StrBSC_Execute pdate=?{$PD_YYYYMMDD} runno=S \
 inp='/WORK/SAP/BSC/BlueSystemCopy/bin/bscc'
 inp=src                  inp=ZT7
 inp=trg                  inp=XT7
 inp=srcdb                inp=degtluv4477
 inp=srcr3                inp=degtluv4477
 inp=trgdb                inp=degtlun5239
 inp=trgr3                inp=degtlun5239
 inp=copymode             inp=force
 inp=keep_initora         inp=NO
 inp=archmode             inp=NO
 inp=from                 inp=tape_offline
 inp=ID                   inp=e0g-154134
 inp=wrkdir               inp='/WORK/SAP/BSC/BlueSystemCopy/wrk'
 inp=logdir               inp='/WORK/SAP/BSC/BlueSystemCopy/client/Integration_BSC_Streamworks/logs'
 inp=logfile              inp='bsc_e0g-154134.log'
 inp=trgstatus            inp=dbup 
 inp=UE                   inp='X,1850,arvatoPrepareRestore.pm bemzaxgd.ffr,bscserv,non-root,rc,0,'
 inp=UE                   inp='X,6510,arvatoRecoverBackup.sh,trgdb,non-root,rc,1,'
 inp=lang                 inp=de
 inp=clientvers           inp=2.7.17.0
 inp=spacecheck           inp=NO
 inp=clicheck             inp=NO
 inp=stopsap              inp=NO
 inp=stopdb               inp=NO
 inp=delfiles             inp=NO
 inp=tsprename            inp=NO
 inp=stopbeforerestore    inp=NO
 inp=delsdm               inp=no
 inp=kernelcheck          inp=no
 inp=async                inp=no
 inp=debug                inp=CHECK;"

]]></MainScript>

          <Script><![CDATA[-- Libraries
--using Streamworks
--
--
-- Dialog variables
completed = false
curPos = 0
msgTxt = ''
regExp = '^[A-Za-z0-9??????!"#$%%&()*+,-./:;<=>?@\\\\^_`{|}~ ]'


Z_STATART = ''
Z_VKBGR = ''
CHK = ''
I = ''

Z_STATKETTE = {}

CHK = string.rep(' ', 80)
I = 0

while Streamworks.IsValid(CHK, 'XX') == 1 do
  I = I + 1
  Z_STATART = ''

  t1 = {}
  while not completed do
    t1[1] = {''}
    t1[2] = {'      Datenkarte : '..Z_VKBGR..'  '..I..' '}
    t1[3] = {'    ============================================'}
    t1[4] = {''}
    t1[5] = {'                 ', Z_STATART, 'o', regExp..'{0,80}$'}
    t1[6] = {''}
    t1[7] = {''}
    t1[8] = {'      XX- ENDE DER EINGABE'}
    if (System.ShowDialog('Dialog', t1, curPos, msgTxt) == false) then
      System.Exit(99, 'User canceled the dialog.')
    end
    Z_STATART = System.ToUpper(t1[5][2])
        
    curPos = 0
    msgTxt = ''

    if Z_STATART == 'XX' then
      CHK = Z_STATART
    else
      Z_STATKETTE[I] = Z_STATART
    end
    break
  end
end
-- Tabelle auslesen und Datenkarten einfügen.
SysJobScript.SetCursorToDD('IEFBR14A.SYSIN')
K=I-1
I=1
while I <= K do
	A = Z_STATKETTE[I]
	SysJobScript.InsertLine(A)
	I=I+1
end]]></Script>

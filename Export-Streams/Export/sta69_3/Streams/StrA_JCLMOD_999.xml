    <StreamPreparationScript>
      <Script><![CDATA[-- Libraries
--using Streamworks
--
-- ABFRAGE DER LAUFVARIANTE (NORMAL, MONATSABSCHLUSS, MONATSWECHSEL)
--
-- Dialog variables
completed = false
curPos = 0
msgTxt = ''
regExp = '^[A-Za-z0-9??????!"#$%%&()*+,-./:;<=>?@\\\\^_`{|}~ ]'


Z_FAKTART = ''
Z_FAKTDAT = ''
Z_FAKTKOMB = ''
Z_VKBGR = ''
CHK = ''
I = ''
N = ''

Z_FAKTKOMB = 7
Z_FAKTKETTE = {}

CHK = string.rep(' ', 2)
I = 0

while Streamworks.IsValid(CHK, 'XX') == 1 do
  I = I + 1
  Z_FAKTART = ''

  t1 = {}
  while not completed do
    t1[1] = {''}
    t1[2] = {'      ABSCHLUSS-FAKTURASTREAM : '..Z_VKBGR..'  '..I..' '}
    t1[3] = {'    ============================================'}
    t1[4] = {''}
    t1[5] = {'                 ', Z_FAKTART, 'o', regExp..'{0,2}$'}
    t1[6] = {''}
    t1[7] = {'      A - STORNO AUFTRAGSFAKTURA'}
    t1[8] = {''}
    t1[9] = {'      XX- ENDE DER EINGABE'}
    if (System.ShowDialog('Dialog', t1, curPos, msgTxt) == false) then
      System.Exit(99, 'User canceled the dialog.')
    end
    Z_FAKTART = System.ToUpper(t1[5][2])
        
    curPos = 0
    msgTxt = ''

    if Streamworks.IsValid(Z_FAKTART, 'A,B,C,D,E,F,G,H,XX,R', 5) == 1 then
      System.Exit(4, 'FALSCHE FAKTURA-ART!')
    end

    if Z_FAKTART == 'XX' then
      CHK = Z_FAKTART
    else
      Z_FAKTKETTE[I] = System.RightTrim(Z_FAKTART)
    end
    break
  end
end
]]></Script>

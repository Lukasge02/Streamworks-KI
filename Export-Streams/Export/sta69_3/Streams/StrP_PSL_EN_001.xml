    <StreamPreparationScript>
      <Script><![CDATA[-- Initialisierungen
t={}
CurPos=0
MsgTxt=''
fertig = false
ArtikelVon = ''
ArtikelBis = ''
-- Beginn der Dialog-Schleife
while (fertig == false) do
   -- Definition der Tabelleninhalte und der regulären Ausdrücke
   t[1] = {'Provide item number range'}
   t[2] = {''}
   t[3] = {'from item number',ArtikelVon,'o','^[0-9]{5}$'}
   t[4] = {'to item number',ArtikelBis,'o','^[0-9]{5}$'}
   -- Anzeige des Dialogfensters
   if (System.ShowDialog('Stock List Catalog',t,CurPos,MsgTxt) == false) then
     -- Der Anwender hat Cancel Button gedrückt und Eingabe abgebrochen  
     System.Exit('99','Input has been canceled by the user')
   else
     ArtikelVon = t[3][2]
     ArtikelBis = t[4][2]
     if (System.ToNumber(ArtikelBis) >= System.ToNumber(ArtikelVon)) then 
         fertig = true 
     else
         -- Cursor in das Eingabefeld 'bis Artikelnummer' stellen
         -- und Fehlermeldung definieren
         CurPos=4
         MsgTxt='Error: to item number must be > or = from item number'
     end 
   end
end
System.SetVariable('ArtikelVon', t[3][2])
System.SetVariable('ArtikelBis', t[4][2])
]]></Script>

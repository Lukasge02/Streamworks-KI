    <StreamPreparationScript>
      <Script><![CDATA[t={}
termine={}
CurPos=0
MsgTxt=''
-- Regulärer Ausdruck für ein Datum von 2000-2099 im Format DD.MM.YYYY
RegExDatum='^((0?[1-9]|[12][0-9]|3[01])\.(0?[13578]|1[02])\.20[0-9]{2}|(0?[1-9]|[12][0-9]|30)\.(0?[13456789]|1[012])\.20[0-9]{2}|(0?[1-9]|1[0-9]|2[0-8])\.(0?[123456789]|1[012])\.20[0-9]{2}|(0?[1-9]|[12][0-9])\.(0?[123456789]|1[012])\.20(00|04|08|12|16|20|24|28|32|36|40|44|48|52|56|60|64|68|72|76|80|84|88|92|96))$'
-- Definition der Tabelleninhalte und der regulären Ausdrücke
t[1]  = {'Liste der Abrechnungstermine'}
t[2]  = {''}
t[3]  = {'Datum 01 ','','m',RegExDatum}
t[4]  = {'Datum 02 ','','m',RegExDatum}
t[5]  = {'Datum 03 ','','m',RegExDatum}
t[6]  = {'Datum 04 ','','m',RegExDatum}
t[7]  = {'Datum 05 ','','m',RegExDatum}
t[8]  = {'Datum 06 ','','m',RegExDatum}
t[9]  = {'Datum 07 ','','m',RegExDatum}
t[10] = {'Datum 08 ','','m',RegExDatum}
t[11] = {'Datum 09 ','','m',RegExDatum}
t[12] = {'Datum 10 ','','m',RegExDatum}
t[13] = {'Datum 11 ','','m',RegExDatum}
t[14] = {'Datum 12 ','','m',RegExDatum}

-- Anzeige des Dialogfensters
if (System.ShowDialog('Abrechnungstermine',t,CurPos,MsgTxt) == false) then
  -- Der Anwender hat Cancel Button gedrückt und Eingabe abgebrochen 
  System.Exit('99','Die Eingabe wurde vom Anwender abgebrochen')
end

-- Tabelle termine mit den eingegebenen Terminen füllen.
i=1
while i < 13 do
   termine[i]={t[2+i][2],'',''}
   termine[i][2]=(string.sub(termine[i][1],1,2))..(string.sub(termine[i][1],4,5))..(string.sub(termine[i][1],7,10))
   termine[i][3]=System.GetInternalDate(termine[i][2],'DDMMYYYY')
   i=i+1
end

-- Tabelle termine ausgeben.
i=1
while i < 13 do
   if i < 10 then
      System.SetVariable('InputTermin0'..i,termine[i][1])
   else
      System.SetVariable('InputTermin'..i,termine[i][1])
   end
   i=i+1
end

-- anhand der Tabelle termine die Termine für die vier Streams StrA_TERMIN_001_01 - 04 setzen
-- StrA_TERMIN_001_01 = vorgegebener Termin - 2 Arbeitstage
-- StrA_TERMIN_001_02 = vorgegebener Termin
-- StrA_TERMIN_001_03 = vorgegebener Termin + 2 Arbeitstage
-- StrA_TERMIN_001_04 = vom vorgegebenen Termin aus auf den nächsten Mittwoch setzen. Wenn der vorgegebene Termin auf einen Mittwoch fällt, dann den vorgegebenen Termin verwenden
i=1
TempDate=''
while i < 13 do
   -- Termin für Stream StrA_TERMIN_001_01 setzen: vorgegebener Termin - 2 Arbeitstage 
   TempDate=System.AddWorkingDay(termine[i][3],'-2')
   System.SetScheduleStatus('StrA_TERMIN_001_01',TempDate,'S')
   -- Termin für Stream StrA_TERMIN_001_02 setzen: vorgegebener Termin 
   System.SetScheduleStatus('StrA_TERMIN_001_02',termine[i][3],'S')
   -- Termin für Stream StrA_TERMIN_001_03 setzen: vorgegebener Termin + 2 Arbeitstage 
   TempDate=System.AddWorkingDay(termine[i][3],'+2')
   System.SetScheduleStatus('StrA_TERMIN_001_03',TempDate,'S')
   -- Termin für Stream StrA_TERMIN_001_04 setzen: vom vorgegebenen Termin aus auf den nächsten Mittwoch (englisch = Wednesday = WE) setzen. 
   --                                              Wenn der vorgegebene Termin auf einen Mittwoch fällt, dann den vorgegebenen Termin verwenden 
   TempDate=System.GetDayOfNextWeek(termine[i][3],'WE')
   System.SetScheduleStatus('StrA_TERMIN_001_04',TempDate,'S')
   i=i+1
end
]]></Script>

          <PreparationScriptJobOrder>0</PreparationScriptJobOrder>
          <Script><![CDATA[------------------------------------------------------------------------------------------------------------------------------------------
-- Zuerst den Programmaufruf definieren und als Inhalt der Variablen ProgAufruf in die PMT schreiben
------------------------------------------------------------------------------------------------------------------------------------------
t={}
t[1]={'Bitte das aufzurufende Programm incl. Pfadangabe angeben'}
t[2]={''}
t[3]={'Pfad + Programm',''}
if (System.ShowDialog('SAP System Kopie per Blue System Copy - Programmaufruf',t,0,'') == false) then
    -- Der Anwender hat Cancel Button gedrückt und Eingabe abgebrochen 
    System.Exit('99','Die Eingabe wurde vom Anwender abgebrochen')
end
System.SetVariable('ProgAufruf',"'"..t[3][2].."'")

------------------------------------------------------------------------------------------------------------------------------------------
-- Nun die Parameter einlesen. 
-- Pro Parameter den Namen und den Inhalt einlesen und in einer Tabelle in einer Tabellenzeile mit zwei Elementen zwischenspeichern.
-- Wenn keine weiteren Parameter mehr kommen (letzte Dialoganzeige mit leeren Feldern + Accept) wird die Schleife beendet
------------------------------------------------------------------------------------------------------------------------------------------
t[1]={'Bitte einen Übergabeparameter definieren'}
t[2]={''}
t[3]={'Parametername  ','initialer Wert'}
t[4]={'Parameterinhalt',''}
p={}
i1=1
while t[3][2] ~= '' do
   -- init Eingabefelder
   t[3][2] = '' 
   t[4][2] = ''
   if (System.ShowDialog('SAP System Kope per Blue System Copy - Parameterdefinition',t,0,'') == false) then
       -- Der Anwender hat Cancel Button gedrückt und Eingabe abgebrochen 
       System.Exit('99','Die Eingabe wurde vom Anwender abgebrochen')
   end
   p[i1]={t[3][2],t[4][2]}
   i1=i1+1
end
i1=i1-1 -- da das letzte Element nicht gefüllt wurde

------------------------------------------------------------------------------------------------------------------------------------------
-- Nun wird die Tabelle zeilenweise verarbeitet
-- Pro Zeile werden die dort enthaltenen Werte zu einem String in der PSL Variablen ScriptText zusammengesetzt.
-- Der 1. Parameter der Zeile beinhaltet den BSC Parameternamen, der 2. Parameter der Zeile beinhaltet den BSC Parameterinhalt
-- Der so gebildete String wird als Inhalt der PMT Variablen ScriptText in die PMT geschrieben.
------------------------------------------------------------------------------------------------------------------------------------------
i2=1
tlen=0
ScriptText=''
-- Liste der Parameter bei denen der Inhalt in Hochkommata gesetzt werden muss
Params={'wrkdir','logdir','logfile','UE','R3UE'}
MaxParams=table.maxn(Params)
while i2 < i1 do
 pos = 1
 -- Bei bestimmten Parameternamen muss der Inhalt in Hochkommata gesetzt werden.
 p1 = 1
 while p1 <= MaxParams do
  if p[i2][1] == Params[p1] then
    p[i2][2]="'"..p[i2][2].."'"
  end
  p1=p1+1
 end
 -- aktuellen Parameter an den bisherigen ScriptText anhängen
 ScriptText=ScriptText..' '..p[i2][1]..'='..p[i2][2]
 i2=i2+1
end
System.SetVariable('ScriptText',ScriptText)]]></Script>

rm StreamworksClient_R?{vReleasePrev}_64Bit.zip
cd $DestinationPath
rm Streamworks.Client.x64.msi
rm &quot;Streamworks Desktop Client Installationshandbuch-*.pdf&quot;

## neue Dateien in temp verschieben
$sourceFile1 = &quot;C:\Program Files\Arvato Systems\Streamworks\Test Streamworks Download\files\Streamworks.Client.x64.msi&quot;
$sourceFile2 = &quot;C:\Program Files\Arvato Systems\Streamworks\Test Streamworks Download\files\Streamworks Desktop Client Installationshandbuch-?{vRelease}.pdf&quot;
Move-Item -Path $sourceFile1 -Destination $DestinationPath
Move-Item -Path $sourceFile2 -Destination $DestinationPath

## neues Zip erstellen
$destinationZip = &quot;C:\Program Files\Arvato Systems\Streamworks\Test Streamworks Download\files\StreamworksClient_R?{vRelease}_64Bit.zip&quot;

try{
	Compress-Archive -Path $DestinationPath -DestinationPath $destinationZip
}
catch {
    Write-host &quot;Error: $($_.Exception.Message)&quot;
	exit 21
}

## restliche Dateien in temp l&#246;schen
cd $DestinationPath
rm *
]]></MainScript>

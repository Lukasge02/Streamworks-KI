. /DBA/nest/streamworks/xbin/sws_setenv
if [ $? -ne 0 ]; then exit 24; fi

WrkDir=/WORK/PASY/PUMGI/International
SpoolFile=$WrkDir/?{$ST_STREAM}_spool.txt

echo &quot;Extract the job name from the spool file ...&quot;
JobName=`grep &quot;Job created:&quot; $SpoolFile | cut -f3 -d&quot;:&quot; | tr -s &quot; &quot; | cut -f2 -d &quot; &quot;`
if [ -z &quot;$JobName&quot; ]
then
  echo &quot;Job name is empty. Error in parsing spool file.&quot;
  exit 222
else
  echo &quot;Job name found: $JobName&quot;
fi

echo &quot;Extract the job id from the spool file ...&quot;
JobId=`grep &quot;Job created:&quot; $SpoolFile | cut -f3 -d&quot;:&quot; | tr -s &quot; &quot; | cut -f3 -d &quot; &quot;`
if [ -z &quot;$JobId&quot; ]
then
  echo &quot;Job id is empty. Error in parsing spool file.&quot;
  exit 222
else
  echo &quot;Job count found: $JobId&quot;
fi

# Aufruf jexa4s Programm
# ----------------------
sws_jexa4s --rc_ext --exec --sid=?{SIDR3} --mdt=?{MANR3} --action=EXE_JOB_MONI  \
 --parm=&quot;EXT_USER:SAPCOMM&quot;                                                      \
 --parm=&quot;MSG_CLASS:ZI&quot;                                                          \
 --parm=&quot;JOB:$JobName&quot;                                                          \
 --parm=&quot;JOB_COUNT:$JobId&quot;



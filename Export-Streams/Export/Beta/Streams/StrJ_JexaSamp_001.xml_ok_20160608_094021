JobInfoFile=$WorkDir/?{$ST_STREAM}.D?{$PD_YYMMDD}MAHN-V.jobinfo

JobInfo=`head -n 1 $JobInfoFile | tail -n 1`
JobName=`echo $JobInfo | cut -f1 -d &quot;#&quot;`
JobId=`echo $JobInfo | cut -f2 -d &quot;#&quot;`

if [ -n &quot;$JobName&quot; -a -n &quot;$JobId&quot; ]
then
  echo &quot;Start Job $JobName with ID $JobId ...&quot;
  /BAGJAS/BAGJAS/PROD/SITE/bin/frame.sh --dba jexa4s_exec.pl           \
  ?{SIDR3}  ?{MANR3}  EXE_JOB_PROCEED                                      \
  EXT_USER:SAPCOMM                            JOB_START:I              \
  JOB:$JobName    JOB_COUNT:$JobId            MSG_CLASS:ZI
  RC=$?
  if [ $RC -eq 8 ]
  then
    AwkInp=/BAGJAS/PROD/ASV/cntl/chk_immediate_start.awk
    ErrHit=`awk -f $AwkInp ${HJRJOBLOG}stderr`
    if [ $ErrHit -eq 2 ]
    then
      echo &quot;Jobstart immediate failed&quot;
      RC=106
    fi
  fi
else
  echo &quot;JobName or JobId not found from run time variable $Laufvar&quot;
  RC=92
fi
exit $RC


set FlamOut=%InpBase%\flamfiles
set FlamLog=%InpBase%\flamfiles\?{$ST_STREAM}_flamlog.txt
set FileList=%InpBase%\flamfiles\?{$ST_STREAM}_dellist.txt

REM if not exist %FlamOut% mkdir %FlamOut%
REM if exist %FlamLog% del %FlamLog%

REM call C:\PASY\PCM\bin\sws_archive_log.cmd        ^
 --inpbase=%InpBase%  --flamout=%FlamOut%           ^
 --flamlog=%FlamLog%                                ^
 --filelist=%FileList%  --mode=joblog
 
 exit 0]]></MainScript>

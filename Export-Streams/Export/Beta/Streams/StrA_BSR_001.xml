-- Wenn in der Auswahl ein SAP System steht, dann dieses SAP System verwenden
if BSR.SAP_Auswahl_01 ~= nil then
   System.SetVariable(&#39;SAP_System&#39;,BSR.SAP_Auswahl_01)
-- Wenn die Auswahl leer ist und der Anwender manuell ein SAP System vorgegeben hat, dann dieses System verwenden
elseif BSR.SAP_Eingabe_01 ~= nil then
   System.SetVariable(&#39;SAP_System&#39;,BSR.SAP_Eingabe_01)
-- Wenn beide Felder leer sind bricht die Aufbereitung ab
else
   System.Exit(&#39;80&#39;,&#39;SAP System Name is missing&#39;)
end

System.SetStartTime(BSR.Stream_Uhrzeit_01)
System.SetStartDate(BSR.Stream_Datum_01)

KD_Datum=System.Format(BSR.Stream_Datum_01,&#39;YYYY-MM-DD&#39;)
KD_Zeit=System.Format(BSR.Stream_Uhrzeit_01,&#39;HH:mm:ss&#39;)
KD_Timezone=string.sub(BSR.Stream_Uhrzeit_01,23,24)

SAP_Datum=System.Format(BSR.SAP_Datum_01,&#39;YYYY-MM-DD&#39;)
SAP_Zeit=System.Format(BSR.SAP_Uhrzeit_01,&#39;HH:mm:ss&#39;)
SAP_Timezone=string.sub(BSR.SAP_Uhrzeit_01,23,24)

if BSR.Warten_01 == &#39;true&#39; then
   KD_Warten=&#39;ja&#39;
else
   KD_Warten=&#39;nein&#39;
end

System.SetVariable(&#39;KD_Datum&#39;,KD_Datum)
System.SetVariable(&#39;KD_Zeit&#39;,KD_Zeit)
System.SetVariable(&#39;KD_Timezone&#39;,KD_Timezone)
System.SetVariable(&#39;SAP_Datum&#39;,SAP_Datum)
System.SetVariable(&#39;SAP_Zeit&#39;,SAP_Zeit)
System.SetVariable(&#39;SAP_Timezone&#39;,SAP_Timezone)
System.SetVariable(&#39;KD_Warten&#39;, KD_Warten)
]]></Script>

    <StreamPreparationScript>
      <Script><![CDATA[-- Initialisierungen
t={}
CurPos=0
MsgTxt=&#39;&#39;
fertig = false
ArtikelVon = &#39;&#39;
ArtikelBis = &#39;&#39;
-- Beginn der Dialog-Schleife
while (fertig == false) do
   -- Definition der Tabelleninhalte und der regul&#228;ren Ausdr&#252;cke
   t[1] = {&#39;Artikelnummernbereich f&#252;r Artikelstammliste vorgeben&#39;}
   t[2] = {&#39;&#39;}
   t[3] = {&#39;von Artikelnummer&#39;,ArtikelVon,&#39;o&#39;,&#39;^[0-9]{5}$&#39;}
   t[4] = {&#39;bis Artikelnummer&#39;,ArtikelBis,&#39;o&#39;,&#39;^[0-9]{5}$&#39;}
   -- Anzeige des Dialogfensters
   if (System.ShowDialog(&#39;Artikelstammliste&#39;,t,CurPos,MsgTxt) == false) then
     -- Der Anwender hat Cancel Button gedr&#252;ckt und Eingabe abgebrochen  
     System.Exit(&#39;99&#39;,&#39;Die Eingabe wurde vom Anwender abgebrochen&#39;)
   else
     ArtikelVon = t[3][2]
     ArtikelBis = t[4][2]
     if (System.ToNumber(ArtikelBis) &gt;= System.ToNumber(ArtikelVon)) then 
         fertig = true 
     else
         -- Cursor in das Eingabefeld &#39;bis Artikelnummer&#39; stellen
         -- und Fehlermeldung definieren
         CurPos=4
         MsgTxt=&#39;FEHLER: bis Artikelnummer muss &gt; oder = von Artikelnummer sein&#39;
     end 
   end
end
System.SetVariable(&#39;ArtikelVon&#39;, t[3][2])

    <StreamPreparationScript>
      <Script><![CDATA[t={}
CurPos=0
MsgTxt=''
fertig = false
Gruppe = ''
-- Zuerst alle Jobs auf inaktiv setzen, in Abh채ngigkeit von de Eingabe im Dialog werden sie dann entsprechend eingeschaltet
System.SetJobRunStatus('00100-FLE03_EXP_IMP_Daten_#D#','n')
System.SetJobRunStatus('00200-FLE03_EXP_IMP_Daten_#D#','n')
System.SetJobRunStatus('00300-FLE03_EXP_IMP_Daten_#D#','n')

-- Beginn der Dialog-Schleife
while (fertig == false) do
   -- Definition der Tabelleninhalte und der regul채ren Ausdr체cke
   t[1] = {'Welche Gruppe soll ausgerollt werden (1, 2 oder 3)'}
   t[2] = {''}
   t[3] = {'Auszurollende Gruppe ist ',Gruppe,'o','^[1-3]{1}$'}
   
   -- Anzeige des Dialogfensters
   if (System.ShowDialog('Welche Gruppe soll ausgerollt werden :1, 2 oder 3?',t,CurPos,MsgTxt) == false) then
     -- Der Anwender hat Cancel Button gedr체ckt und Eingabe abgebrochen  
     System.Exit('99','Die Eingabe wurde vom Anwender abgebrochen')
   else
     Gruppe = t[3][2]
        if (System.ToNumber(Gruppe) == 1) then 
         fertig = true 
		 System.SetJobRunStatus('00100-FLE03_EXP_IMP_Daten_#D#','y')
        else
         if (System.ToNumber(Gruppe) == 2) then
			fertig = true 
			System.SetJobRunStatus('00200-FLE03_EXP_IMP_Daten_#D#','y')
		 else
			if (System.ToNumber(Gruppe) == 3) then
				fertig = true 
				System.SetJobRunStatus('00300-FLE03_EXP_IMP_Daten_#D#','y')
			else
			-- Cursor in das Eingabefeld 'Auszurollende Gruppe' stellen
			-- und Fehlermeldung definieren
			CurPos=4
			MsgTxt='FEHLER: Die angegebene Gruppe gibt es nicht'
			end
	      end		
		end 
   end
end]]></Script>
      <ScriptLanguage>Lua</ScriptLanguage>
    </StreamPreparationScript>

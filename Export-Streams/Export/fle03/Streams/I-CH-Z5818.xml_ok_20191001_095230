	LoginObjectRequired = &#39;false&#39;
	AutoStart = &#39;false&#39;
	Agt = &#39;&#39;
	VA = &#39;&#39;
	if Desc == NIL then
		Desc = AgentIP
	end

-- ----------------------------------------------------------------------- --
-- Negieren aller Jobs --
-- ----------------------------------------------------------------------- --
	System.SetJobRunStatus(&#39;Z5818010&#39;, &#39;n&#39;)
	System.SetJobRunStatus(&#39;Z5818020&#39;, &#39;n&#39;)
	System.SetJobRunStatus(&#39;Z5818030&#39;, &#39;n&#39;)
	System.SetJobRunStatus(&#39;Z5818040&#39;, &#39;n&#39;)
	System.SetJobRunStatus(&#39;Z5818045&#39;, &#39;n&#39;)
	System.SetJobRunStatus(&#39;Z5818050&#39;, &#39;n&#39;)

-- ----------------------------------------------------------------------- --
-- AgentName nicht identisch AgentIP --   
-- ----------------------------------------------------------------------- --	
	if AgentName == NIL then
		AgentName = AgentIP
	else 
		AgentName = AgentName
	end		
	
-- ----------------------------------------------------------------------- --
-- Cluster Auswahl oder neuer Cluster --   
-- ----------------------------------------------------------------------- --	
	New_Cluster = BSR.NEW_CLUSTER
	if New_Cluster == NIL then
		Cluster = Cluster
	else
		Cluster = New_Cluster
	end

-- ----------------------------------------------------------------------- --
-- Auswahl Virtuellen- oder Festen- Agenten --   
-- ----------------------------------------------------------------------- --
	if VirtAgent == &#39;true&#39; then
		Virtual = &#39;true&#39;
		Agt = &#39;V-&#39;
	else
		AutoStart = &#39;true&#39;
		if I200 == &#39;true&#39; then
			VA = &#39;!!!!! Achtung!!!!! Bei Belgischen Agenten muss das Login Objekt required aktiviert sein!!!!! Dies l&#228;sst sich zur Zeit noch nicht automatisieren!&#39;
		else
			VA = &#39;&#39;
		end
	end		
	
-- ----------------------------------------------------------------------- --
-- Jira oder Mail an Auftraggeber zuweisen  --   
-- ----------------------------------------------------------------------- --   
	OSLocale = &#39;&#39;
	if (Type == &#39;UNIX&#39; and VirtAgent == &#39;true&#39;) then
		OSLocale = &#39;utf-8&#39;
		VA = &#39;     !!Dies ist ein Virtueller Agent es muss kein Agent bestellt werden!!     &#39;
		VAFW = &#39;     !!Dies ist ein Virtueller Agent bitte Pr&#252;fen ob es einen Firewall Auftrag braucht!!     &#39;
		User = System.GetSystemVariable(&#39;$PR_USER&#39;)..&#39;@baloise.com&#39;
	elseif (Type == &#39;WINDOWS&#39; and VirtAgent == &#39;true&#39;) then
		OSLocale = &#39;Windows-1252&#39;
		VA = &#39;     !!Dies ist ein Virtueller Agent es muss kein Agent bestellt werden!!     &#39;
		VAFW = &#39;     !!Dies ist ein Virtueller Agent bitte Pr&#252;fen ob es einen Firewall Auftrag braucht!!     &#39;
		User = System.GetSystemVariable(&#39;$PR_USER&#39;)..&#39;@baloise.com&#39;
		-- elseif Type == &#39;Unix&#39;		Jira-Auftrag --
	elseif Type == &#39;UNIX&#39; then
--		User = &#39;group_ch_dcs_pikettantraege@baloise.ch&#39; --
		User = System.GetSystemVariable(&#39;$PR_USER&#39;)..&#39;@baloise.com&#39;
-- else 					Mail an Auftraggeber --		
	else
		User = System.GetSystemVariable(&#39;$PR_USER&#39;)..&#39;@baloise.com&#39;
	end


-- ----------------------------------------------------------------------- --
-- Serverendpoint zuweisen  --   
-- ----------------------------------------------------------------------- -- 
	System.SetVariable(&#39;SRVPOINT&#39;, &#39;streamw-proc1-i.balgroupit.com&#39;)


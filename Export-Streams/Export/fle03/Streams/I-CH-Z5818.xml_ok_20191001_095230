        <MainScript><![CDATA[rem Jobname        = ?{$ST_JOB}
rem Streamname     = ?{$ST_STREAM}
rem Variante       = ?{$ST_VARI}
rem Lauf-Nr.       = ?{$ST_RUN}
rem Plandatum      = ?{$PR_DDMMYY}
rem Startdatum     = ?{&amp;SJ_DDMMYY}

rem --  IP-Adresse aus DNS-Namen ermitteln und in Variable IPADRESSE schreiben --

ping ?{AgentIP} &amp;&amp; for /f &quot;tokens=2&quot; %%a in (&#39;nslookup ?{AgentIP}&#39;) do set IPADRESSE=%%a

rem --  Firewall Gruppe festlegen --

echo.%IPADRESSE% | FIND /I &quot;10.64.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=corp-dc.streamwork-agents-i-dcshared.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.26.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=chbv-dc.streamwork-agents-i.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.76.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=chbv-dc.streamwork-agents-i.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;172.16.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=chbv-sv.streamwork-agents-i-mvshost.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.253.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=bebv-dc.streamwork-agents-i.ng	 ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.252.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=bebv-dc.np.streamwork-agents-i.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.66.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=corp-dc.np.streamwork-agents-i.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.77.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=chbv-dc.np.streamwork-agents-i.ng ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.231.128.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=pan-soprasteria.streamwork-agents-i.ng  ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.231.129.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=pan-soprasteria.streamwork-agents-i.ng  ) || ( Echo.Nicht gefunden )
echo.%IPADRESSE% | FIND /I &quot;10.249.&quot;&gt;Nul &amp;&amp; ( set FIREWALLGRP=debv-dc.streamwork-agents-i.ng ) || ( Echo.Nicht gefunden )

if &quot;%FIREWALLGRP%&quot;==&quot;&quot; (set FIREWALLGRP=Keine zuweisung gefunden)

rem -- Streamworks Uservariable setzen

StreamworksAgentCLI --port 30151 --cliuser CLI_Batch --clicmd &quot;func=USERVAR varname=&amp;&amp;I150-?{$ST_JOB}-IPADR varvalue=%IPADRESSE% desc=Server_IP-Adresse doc=IP-Adresse_aus_Server-Namen action=U&quot;
StreamworksAgentCLI --port 30151 --cliuser CLI_Batch --clicmd &quot;func=USERVAR varname=&amp;&amp;I150-?{$ST_JOB}-FWGRP varvalue=%FIREWALLGRP% desc=FirewallGruppe doc=Firewall_Gruppe_zuordnen action=U&quot;


rem -- Anzeigen gewaehlte Firewallgruppe --

@echo.%FIREWALLGRP%

rem -- Varaible IPADRESSE wieder loeschen --

set FIREWALLGRP=
set IPADRESSE=]]></MainScript>

	BSR = System.GetFormTable(&#39;F_&#39;..System.GetSystemVariable(&#39;$ST_STREAM&#39;))
	
-- Eingabewerte &#252;bernehmen / ausgeben --

-- ----------------------------------------------------------------------- --
-- Variablen init --   
-- ----------------------------------------------------------------------- --
	AgentIP = BSR.AgentIP
	AgentName = BSR.AgentName
	Type = BSR.Type
	Desc = BSR.Description
	I100 = BSR.Mandant_I100
	I150 = BSR.Mandant_I150
	I200 = BSR.Mandant_I200
	I250 = BSR.Mandant_I250
	I300 = BSR.Mandant_I300
	I350 = BSR.Mandant_I350

    <StreamPreparationScript>
      <Script><![CDATA[-- Eine while Schleife mit 5 Durchl&#228;ufen erstellen. 
-- Pro Durchlauf wird eine Z&#228;hlervariable um 1 erh&#246;ht. 
-- Der Inhalt der Z&#228;hlervariablen wird pro Durchlauf in die PMT gest.
-- Variablenname   = &#39;TestVar_n&#39; (n = Inhalt der Z&#228;hlervariablen)
-- Variableninhalt = Inhalt der Z&#228;hlervariablen.
i=0
while i&lt;5 do
  i=i+1 
  System.SetVariable(&#39;TestVar_&#39;..i,i)
end

-- Eine Tabelle t mit f&#252;nf Elementen erstellen. 
-- pro Tabellenelement eine Untertabelle mit jeweils zwei Elementen,
-- 1. Untertabellenelem. hat den Text &#39;TabVar_n&#39;
-- 2. Untertabellenelem. hat den Text &#39;Inhalt von TabVar Nummer n&#39;
-- F&#252;r n die Werte 1 (im 1. Element) – 5 (im 5. Element) eintragen
t={}
t[1]={&#39;TabVar_1&#39;,&#39;Inhalt von TabVar Nummer 1&#39;}
t[2]={&#39;TabVar_2&#39;,&#39;Inhalt von TabVar Nummer 2&#39;}
t[3]={&#39;TabVar_3&#39;,&#39;Inhalt von TabVar Nummer 3&#39;}
t[4]={&#39;TabVar_4&#39;,&#39;Inhalt von TabVar Nummer 4&#39;}
t[5]={&#39;TabVar_5&#39;,&#39;Inhalt von TabVar Nummer 5&#39;}
-- Die 5 Tabellenelemente der Tabelle t in die PMT stellen, dabei 
-- jeweils das erste Untertabellenelement als Variablenname und das 
-- zweite Untertabellenelement als Variableninhalt verwenden.
System.SetVariable(t[1][1],t[1][2])
System.SetVariable(t[2][1],t[2][2])
System.SetVariable(t[3][1],t[3][2])
System.SetVariable(t[4][1],t[4][2])
System.SetVariable(t[5][1],t[5][2])

-- Eine Tabelle t1 mit Hilfe einer while Schleife f&#252;llen. n dabei mit
-- dem Inhalt einer Z&#228;hlervariablen f&#252;llen 
-- 1. Untertabellenelement hat den Text &#39;WTabVar_n&#39;
-- 2. Untertabellenelement hat den Text &#39;Inhalt von WTabVar Nummer n&#39;
t1={}
i=0
while i&lt;5 do
  i=i+1 
  t1[i]={&#39;WTabVar_&#39;..i,&#39;Inhalt von WTabVar Nummer &#39;..i}
end

-- Danach die Inhalte der Tabelle t1 mit einer repeat Schleife 
-- auslesen und in f&#252;nf verschiedene Variablen in die PMT ausgeben.
-- Dabei jeweils das erste Untertabellenelement als Variablenname und
-- das zweite Untertabellenelement als Variableninhalt verwenden.
i=0
repeat
  i=i+1 
  System.SetVariable(t1[i][1],t1[i][2])
until i&gt;=5

]]></Script>
      <ScriptLanguage>Lua</ScriptLanguage>

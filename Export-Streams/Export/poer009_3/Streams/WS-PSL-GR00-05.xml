       System.Exit(&#39;99&#39;,&#39;Abbruch&#39;) 
   end
   System.SetVariable(&#39;Ort&#39;,t1[1][2])
else  -- Auswahl = &#39;e&#39;
   System.SetVariable(&#39;Ende&#39;,&#39;Kein Dialog gew&#252;nscht&#39;)
end   

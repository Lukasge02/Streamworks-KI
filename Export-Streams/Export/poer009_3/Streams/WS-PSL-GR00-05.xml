      System.Exit(&#39;99&#39;,&#39;Abbruch&#39;) 
   end
   System.SetVariable(&#39;Name&#39;,t1[1][2])
elseif t[3][2] == &#39;2&#39; then -- Auswahl = &#39;2&#39; – Dialog 2 anzeigen
   t1[1] = {&#39;Ort:&#39;,&#39;&#39;}
